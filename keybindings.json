[
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+tab",
        "command": "editor.action.formatSelection",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.tasks.runTask"
    }
]